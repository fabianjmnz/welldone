            <div class="col-10">
                <p class="author-category-text">
                    {{Author}} in {{Category}}
                </p>
                <p class="post-secondary-text text-secondary">
                    {{createdAt}} - 8 min read <span class="favortie-article-heart">&hearts;</span>
                </p>
            </div>
            <div class="col-1 bookmark">
                <p>
                    <span class="bookmark-icon">&#9750;</span>
                </p>
            </div>
        </div>
    </div>
    <div class="col-4">
        <img src="./images/pcImage.jpg">
